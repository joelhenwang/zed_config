{
  "HTML5 Boilerplate": {
    "prefix": "html",
    "body": [
      "<!DOCTYPE html>",
      "<html lang=\"${1:en}\">",
      "<head>",
      "\t<meta charset=\"UTF-8\">",
      "\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
      "\t<title>${2:Document}</title>",
      "</head>",
      "<body>",
      "\t$0",
      "</body>",
      "</html>"
    ]
  },
